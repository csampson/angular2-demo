{
  "extends": "airbnb-base",
  "rules": {
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "no-cond-assign": [2, "except-parens"],
    "no-unused-vars": [1, { "vars": "local", "args": "none" }],
    "default-case": 0,
    "no-param-reassign": 0,
    "no-multi-spaces": [2, { "exceptions": { "ImportDeclaration": true } }]
  },
  
    "experimentalDecorators": true
  
}
